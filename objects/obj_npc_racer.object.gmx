    bonusAcc = 1;
}
else {
    bonusAcc = 1.3;
}
if(!instance_exists(obj_ready) &amp;&amp; y &gt; obj_finish_line.y){
    if(spd &lt; global.maxSpeed){
        spd += (sin(degtorad(0.6/(gear*0.5)))*(2.35-friction))*bonusAcc;
    }
}
else if (y &lt; obj_finish_line.y){
    spd -= tan(degtorad(2*(5-(friction+2))));
    condition = 25;
}
if(spd &lt;= 0) {spd = 0}
