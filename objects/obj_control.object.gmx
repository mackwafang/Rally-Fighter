    if(a == 0){ //  First value
        scoreMove[0] = var_wipe(scoreMove[0],floor(score),100,0.5,0);
    }
    else{ //Other Values
        scoreMove[a] = var_wipe(scoreMove[a],round(scoreMove[a-1]) div 10,100,0.2,0);
